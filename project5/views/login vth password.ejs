</head>

<body style="background-image:url(/images/img1.jpg);">


  <div class="head">
    <img id="logo" src="/images/cdac logo.jpg" alt="">
    <div class="title">
      <h1> Centre for Development of Advanced Computing Center in North East  SILCHAR</h1>
  
      <h3 style="font-size:30px;"> Public Grievance Redressal System</h3>
    </div>
    <img id="clg" src="/images/WhatsApp Image 2022-12-19 at 3.45.10 PM.jpeg" alt="">
  </div>
  <div class="nav">
    <a href="/" class="nav">HOME</a>
    <a href="/about" class="nav">ABOUT US</a> 
      <a href="/contact" class="nav">CONTACT US</a>
   
      <a href="https://www.cdac.in/index.aspx?id=edu_acts_NECertificateSilchar">C-DAC</a>
  
    </div>
  <button class="tablink" onclick="openPage('Userlogin', this,'rgb(96 107 178 / 40%)')" id="defaultOpen">USER LOGIN USING PASSWORD</button>
  <button class="tablink" onclick="openPage('otplogin', this, '#8f443b7a')">USER LOGIN USING OTP</button>

  <% if(message.length > 0){ %> 
    <div class="alert">
      <%= message %> 
    </div>
  <% } %>

  <div id="Userlogin" class="tabcontent">

    <h2 class="heading">
      Login Using Password
    </h2>

    <!-- images -->
    <div class="wrapper">

      <div class="images-area">
        <img src="/images/img6.jpg" alt="image"class="firstImage">
        <img src="/images/img4.jpg" alt="image">
        <img src="/images/img5.jpg" alt="image">

        <img src="/images/cdac1.jpeg" alt="image" >
        
        <img src="/images/cdac2.jpeg" alt="image">
        <img src="/images/cdac3.jpeg" alt="image">
        <img src="/images/cdac4.jpeg" alt="image">
       
        
      </div>

      <div class="buttons-area">
        <div class="previous-btn">
          <i class='bx bx-chevron-left'></i>
        </div>
        <div class="next-btn">
          <i class='bx bx-chevron-right'></i>
        </div>
      </div>

      <div class="pagination-area">
      </div>
    </div>
    <!-- images end -->


    <form class="form" action="/login" method="post">
      <div class="middle1">
        <p>
          Username/Email Id/Mobile No
        </p>
        <input type="text" name="username" placeholder="Username"><br>
        <p>
          Password
        </p>
        <input type="password" name="password" placeholder="password"><br>
        <div class="login-form">
          <div class="captcha">
            <label for="captcha-input">Security Code</label>
            <input type="text" id="captcha-form" placeholder="Security Code">
         
              
              <div class="button">
                <div class="preview"></div>
                <input type="button" class="captcha-refresh" >
                <i class="fa fa-refresh"></i>
              </input>
              </div>
           
          </div>
        </div>
      </div>

      <br><br>
      <div class="div1">
        <button type="submit">Login</button>
        <!-- <button onclick="reset()">Reset</button> -->
      </div>
      <div class="link1">

        <ul>

          <li><a href="/forgotPass">Forgot password?</a></li>
          <li><a href="/forgotUsername">Forgot username?</a></li>

          <li><a href="/registration">Not a member? Register</a></li>
        </ul>
      </div>
    </form>
  </div>


  <div id="otplogin" class="tabcontent">

    <h2 class="heading">
      LOGIN USING OTP
    </h2>


    <div class="wrapper1">

      <div class="imagesarea1">
        <img src="/images/img3.jpg" alt="image" class="firstImage1">


      </div>

      <div class="buttonsarea1">
        <div class="previousbtn1">
          <i class='bxbxchevronleft1'></i>
        </div>
        <div class="nextbtn1">
          <i class='bxbxchevronright1'></i>
        </div>
      </div>

      <div class="paginationarea1">
      </div>
    </div>




    <form class="form1" method="post">
      <div class="middle2">
        <p>
          Username/Email Id/Mobile No
        </p>
        <input type="text" name="user" placeholder="Username"><br>
        <p>
          OTP
        </p>
        <div class="otp">

          <input type="OTP" name="OTP" placeholder="Password"><br>

          <button type="button" onclick="generate()">Generate OTP</button>


        </div>
      </div>
      <br><br>
      <div class="div1">
        <button type="submit">Login</button>
        <!-- <button onclick="reset()">Reset</button> -->
      </div>
      <div class="link2">

        <ul>

          <li><a href="/forgotPass">Forgot password?</a></li>
          <li><a href="/forgotUsername">Forgot username?</a></li>

          <li><a href="/registration">Not a member? Register</a></li>
        </ul>
      </div>

    </form>
  </div>
